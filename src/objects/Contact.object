    <validationRules>
        <fullName>Inactive_contact_cannot_be_primary</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Primary_Contact__c = true,  Inactive_Contact__c =TRUE)</errorConditionFormula>
        <errorDisplayField>Inactive_Contact__c</errorDisplayField>
        <errorMessage>An Inactive contact cannot be a Primary Contact simultaneously.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_number_can_only_contain_digits</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR( 
IF( 
ISBLANK(Phone) , false , NOT(REGEX(Phone, &quot;0\\D*?(\\d\\D*?){10}&quot;))), 
IF( 
ISBLANK(MobilePhone) , false , NOT(REGEX(Phone, &quot;0\\D*?(\\d\\D*?){10}&quot;)))),
$Profile.Name != &apos;API User Profile&apos;)</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>Please ensure the number starts with a 0 and is 11 digits long</errorMessage>
    </validationRules>
