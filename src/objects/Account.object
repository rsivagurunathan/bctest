    <validationRules>
        <fullName>Billing_address_cannot_be_blank</fullName>
        <active>true</active>
        <description>The billing address is mandatory</description>
        <errorConditionFormula>IF(
ISNEW(),
FALSE,OR(ISBLANK( BillingCity ), 
ISBLANK( BillingStreet ), 
ISBLANK( BillingPostalCode)))</errorConditionFormula>
        <errorDisplayField>BillingStreet</errorDisplayField>
        <errorMessage>Please enter a billing address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Check_Passed</fullName>
        <active>true</active>
        <description>When finance informs the sales team that the account passed the credit check, the SAP ID must be populated.</description>
        <errorConditionFormula>AND(( Credit_Check_Passed__c = TRUE), ISBLANK( SAP_ID__c ))</errorConditionFormula>
        <errorDisplayField>SAP_ID__c</errorDisplayField>
        <errorMessage>Please enter the SAP ID for this account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_number_can_only_contain_digits</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(REGEX(Phone, &quot;0\\D*?(\\d\\D*?){10}&quot;))
,$Profile.Name != &apos;API User Profile&apos;)</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>Please ensure the number starts with a 0 and is 11 digits long</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAP_ID_starts_with_a_9_and_be_7_digits</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(REGEX( SAP_ID__c , &quot;9\\D*?(\\d\\D*?){6}&quot;)) 
,$Profile.Name != &apos;API User Profile&apos;)</errorConditionFormula>
        <errorDisplayField>SAP_ID__c</errorDisplayField>
        <errorMessage>This SAP ID is invalid, all SAP IDs start with 9</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UK_Postcode_format</fullName>
        <active>true</active>
        <description>Checks that the post code is a UK format  of letters and digits (LL11 1LL)</description>
        <errorConditionFormula>IF( 
ISNEW(), 
FALSE,NOT(OR(

ISNULL( BillingPostalCode ),

REGEX( BillingPostalCode , &quot;[a-zA-Z]\\d\\s\\d[a-zA-Z]{2}&quot;),

REGEX(BillingPostalCode , &quot;[a-zA-Z]\\d\\d\\s\\d[a-zA-Z]{2}&quot;),

REGEX( BillingPostalCode , &quot;[a-zA-Z]{2}\\d\\s\\d[a-zA-Z]{2}&quot;),

REGEX( BillingPostalCode , &quot;[a-zA-Z]{2}\\d\\d\\s\\d[a-zA-Z]{2}&quot;),

REGEX(BillingPostalCode , &quot;[a-zA-Z]{2}\\d[a-zA-Z]\\s\\d[a-zA-Z]{2}&quot;),

REGEX( BillingPostalCode , &quot;[a-zA-Z]\\d[a-zA-Z]\\s\\d[a-zA-Z]{2}&quot;)

)))</errorConditionFormula>
        <errorDisplayField>BillingPostalCode</errorDisplayField>
        <errorMessage>Please ensure the format for this postcode is correct (LL11 1LL)</errorMessage>
    </validationRules>
